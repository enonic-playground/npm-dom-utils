{
  "name": "@enonic/js-utils",
  "description": "Dom Utils",
  "version": "0.0.0",
  "author": "Christian Westgaard <cwe@enonic.com>",
  "contributors": [],
  "devDependencies": {
    "@jest/globals": "^29",
    "concurrently": "^8",
    "del-cli": "^5",
    "glob": "^10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29",
    "ts-jest": "^29",
    "ts-node": "^10",
    "tsup": "^7",
    "typescript": "^5"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "dom",
    "utils",
    "es2015",
    "ecmascript",
    "javascript",
    "typescript"
  ],
  "license": "Apache-2.0",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:enonic/npm-dom-utils.git"
  },
  "scripts": {
    "build": "concurrently -c auto -g --timings npm:build:*",
    "build:cjs": "npx tsup -d dist/cjs --silent",
    "build:esm": "npx tsup -d dist/esm --silent",
    "check:types": "npx tsc --noEmit",
    "clean": "del dist",
    "prebuild": "npm run check:types",
    "precheck:types": "npm run clean",
    "test": "jest --no-cache --coverage"
  },
  "type": "module",
  "types": "index.d.ts"
}
